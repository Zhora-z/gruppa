
#include <iostream>
using namespace std;
int main(){
  cout << "Введите 1 число" << endl;
  int a;
  cin >> a;
  cout << "Введите 2 число" << endl;
  int b;
  cin >> b;
  cout << "Введите 3 число" << endl;
  int c;
  cin >> c;
  if (a > b){
    if (a > c){
      cout << "Первое число наибольшее  " << a << endl;
    }
  }
  if (b > a){
    if (b > c){
      cout << "Второе число наибольшее  " << b << endl;
    }
    
  }
  if (c > a){
    if (c > b){
           cout << "Третье число наибольшее  " << c << endl;
    }
  }
}
